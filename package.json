{
  "name": "@materialr/toolbar",
  "version": "0.0.0-development",
  "description": "Material toolbar implementation for React",
  "main": "lib/index.js",
  "scripts": {
    "build:production": "babel src --out-dir lib --ignore **/*.test.js,**/*.test.jsx",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "lint": "eslint --ext .js .",
    "semantic-release": "semantic-release",
    "test": "npm run lint && npm run test:scripts",
    "test:scripts": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/materialr/toolbar.git"
  },
  "keywords": [
    "React",
    "Material",
    "Toolbar"
  ],
  "author": "Hendrik Volschenk <righteous.trespasser@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/materialr/toolbar/issues"
  },
  "homepage": "https://github.com/materialr/toolbar#readme",
  "dependencies": {
    "@material/toolbar": "^0.27.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^21.2.1",
    "react-dom": "^16.2.0",
    "semantic-release": "^9.1.1"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.jest"
    ],
    "moduleNameMapper": {
      "\\.scss$": "<rootDir>/.jest/style-mock.js"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "<rootDir>/.jest/shim.js",
      "<rootDir>/.jest/test-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.jest"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(!@material)"
    ]
  }
}
